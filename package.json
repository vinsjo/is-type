{
  "name": "x-is-type",
  "version": "1.0.15",
  "description": "A minimal package for simple type-checks of basic types",
  "type": "module",
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "umd:main": "./dist/index.umd.js",
  "exports": {
    "require": "./dist/index.cjs",
    "import": "./dist/index.mjs",
    "default": "./dist/index.modern.js"
  },
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch",
    "update-browserlist": "npx browserslist@latest --update-db -y",
    "prepublishOnly": "npm run update-browserlist && npm run build"
  },
  "files": [
    "dist"
  ],
  "devDependencies": {
    "microbundle": "^0.14.2"
  },
  "author": "Vincent Sj√∂gren",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsjo/x-is-type"
  },
  "keywords": [
    "ESM",
    "CommonJS",
    "typecheck"
  ]
}
