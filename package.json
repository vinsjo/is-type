{
    "name": "x-is-type",
    "version": "1.2.1",
    "description": "A minimal package for simple type-checks of basic types",
    "type": "module",
    "source": "./src/index.ts",
    "main": "./dist/index.cjs",
    "module": "./dist/index.mjs",
    "umd:main": "./dist/index.umd.js",
    "types": "./dist/index.d.ts",
    "exports": {
        "require": "./dist/index.cjs",
        "import": "./dist/index.mjs",
        "default": "./dist/index.modern.js"
    },
    "scripts": {
        "build": "microbundle src/index.ts",
        "dev": "microbundle watch",
        "lint": "eslint src/** __tests__/** --ext .ts",
        "format": "prettier --write .",
        "test": "jest",
        "test:coverage": "jest --coverage",
        "pre-commit": "npm test && pretty-quick --staged",
        "pre-push": "npm test && npm run lint",
        "prepublishOnly": "npm run pre-push && npm run build",
        "prepare": "husky install"
    },
    "files": [
        "src",
        "dist"
    ],
    "devDependencies": {
        "@jest/globals": "^29.3.1",
        "@typescript-eslint/eslint-plugin": "latest",
        "@typescript-eslint/parser": "latest",
        "eslint": "latest",
        "husky": "^8.0.0",
        "microbundle": "latest",
        "prettier": "^2.8.0",
        "pretty-quick": "^3.1.3",
        "ts-jest": "^29.0.3"
    },
    "author": "Vincent Sj√∂gren",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/vinsjo/x-is-type"
    },
    "keywords": [
        "ESM",
        "CommonJS",
        "typecheck"
    ]
}
