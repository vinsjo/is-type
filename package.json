{
  "name": "x-is-type",
  "version": "1.1.3",
  "description": "A minimal package for simple type-checks of basic types",
  "type": "module",
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "umd:main": "./dist/index.umd.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.modern.js"
    },
    "./callbacks": {
      "require": "./dist/callbacks.cjs",
      "import": "./dist/callbacks.mjs",
      "default": "./dist/callbacks.modern.js"
    }
  },
  "typesVersions": {
    "*": {
      "callbacks": [
        "./dist/callbacks.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "microbundle src/*.ts",
    "dev": "microbundle watch",
    "lint": "npx eslint src --ext .ts",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "src",
    "dist"
  ],
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "eslint": "latest",
    "microbundle": "latest"
  },
  "author": "Vincent Sj√∂gren",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vinsjo/x-is-type"
  },
  "keywords": [
    "ESM",
    "CommonJS",
    "typecheck"
  ]
}